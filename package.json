{
  "name": "pgyer-upload",
  "description": "Upload iOS / Android / HarmonyOS builds to PGYER",
  "version": "0.0.2",
  "author": "vincet",
  "bin": {
    "pgyer-upload": "./bin/run.js"
  },
  "bugs": "https://github.com/cli/pgyer-upload/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/inquirer": "^9.0.9",
    "axios": "^1.13.1",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.4",
    "inquirer": "^12.10.0",
    "nodemailer": "^7.0.10",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@types/node": "^18",
    "@types/nodemailer": "^7.0.3",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/cli/pgyer-upload",
  "keywords": [
    "pgyer",
    "upload",
    "ios",
    "android",
    "harmonyos",
    "ipa",
    "apk",
    "hap",
    "cli"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "pgyer-upload",
    "dirname": "pgyer-upload",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "cli/pgyer-upload",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "ts-node --esm ./bin/dev.js",
    "watch": "tsc -b -w",
    "start": "node ./bin/run.js",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
